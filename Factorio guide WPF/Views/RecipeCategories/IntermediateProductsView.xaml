            <Button Grid.Row="1" Grid.Column="1" Margin="1,1,1,1" BorderThickness="0">
                <Image Source="/Views/RecipeCategories/components/48px-Heavy_oil.png" />
            </Button>
            <Button Grid.Row="1" Grid.Column="2" Margin="1,1,1,1" BorderThickness="0">
                <Image Source="/Views/RecipeCategories/components/48px-Light_oil.png" />
            </Button>
            <Button Grid.Row="1" Grid.Column="3" Margin="1,1,1,1" BorderThickness="0">
