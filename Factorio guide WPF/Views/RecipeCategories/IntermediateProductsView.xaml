                <Image Source="/Views/RecipeCategories/components/48px-Wood.png" />
            </Button>
            <Button Grid.Row="2" Grid.Column="1" Margin="1,1,1,1" BorderThickness="0">
                <Image Source="/Views/RecipeCategories/components/48px-Coal.png" />
            </Button>
            <Button Grid.Row="2" Grid.Column="2" Margin="1,1,1,1" BorderThickness="0">
                <Image Source="/Views/RecipeCategories/components/48px-Stone.png" />
            </Button>
            <Button Grid.Row="2" Grid.Column="3" Margin="1,1,1,1" BorderThickness="0">
